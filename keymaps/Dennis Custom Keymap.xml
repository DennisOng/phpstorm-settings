<keymap version="1" name="Dennis Custom Keymap" parent="$default">
  <action id="ActivateTerminalToolWindow">
    <keyboard-shortcut first-keystroke="alt f12" />
    <keyboard-shortcut first-keystroke="alt t" />
    <keyboard-shortcut first-keystroke="ctrl t" />
  </action>
  <action id="Debugger.AddToWatch">
    <keyboard-shortcut first-keystroke="shift ctrl w" />
  </action>
  <action id="Debugger.Tree.AddToWatches">
    <keyboard-shortcut first-keystroke="shift ctrl w" />
  </action>
  <action id="EditorDeleteLine">
    <keyboard-shortcut first-keystroke="ctrl y" />
    <keyboard-shortcut first-keystroke="ctrl back_space" />
  </action>
  <action id="EditorDeleteToWordStart" />
  <action id="EditorIndentLineOrSelection">
    <keyboard-shortcut first-keystroke="shift ctrl q" />
  </action>
  <action id="EditorUnSelectWord" />
  <action id="FileStructurePopup">
    <keyboard-shortcut first-keystroke="ctrl f12" />
    <keyboard-shortcut first-keystroke="alt r" />
  </action>
  <action id="GotoTest" />
  <action id="Images.ShowThumbnails" />
  <action id="Jdbc.RunSqlScript">
    <keyboard-shortcut first-keystroke="shift ctrl f10" />
    <keyboard-shortcut first-keystroke="shift ctrl t" />
  </action>
  <action id="RunClass">
    <keyboard-shortcut first-keystroke="shift ctrl f10" />
    <keyboard-shortcut first-keystroke="shift ctrl t" />
  </action>
  <action id="SwitchApply">
    <keyboard-shortcut first-keystroke="shift alt enter" />
  </action>
  <action id="SwitchDown">
    <keyboard-shortcut first-keystroke="shift alt down" />
  </action>
  <action id="SwitchLeft">
    <keyboard-shortcut first-keystroke="shift alt left" />
  </action>
  <action id="SwitchRight">
    <keyboard-shortcut first-keystroke="shift alt right" />
  </action>
  <action id="SwitchUp">
    <keyboard-shortcut first-keystroke="shift alt up" />
  </action>
  <action id="Vcs.UpdateProject" />
</keymap>